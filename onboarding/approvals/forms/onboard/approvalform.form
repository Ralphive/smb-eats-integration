{
    "id": "approvalform",
    "schemaVersion": "1.1.0",
    "name": "approvalform",
    "revision": "1.0",
    "type": "task",
    "readOnly": false,
    "content": [
        {
            "id": "first_name",
            "type": "string",
            "label": "First Name",
            "readOnly": false,
            "value": "${context.empData.firstName}",
            "constraints": {
                "required": false
            }
        },
        {
            "id": "last_name",
            "type": "string",
            "label": "Last Name",
            "readOnly": false,
            "value": "${context.empData.lastName}",
            "constraints": {
                "required": false
            }
        },
        {
            "id": "country_",
            "type": "string",
            "label": "Address",
            "readOnly": false,
            "value": "${context.empData.fullAddress}",
            "constraints": {
                "required": false
            }
        },
        {
            "id": "hire_date",
            "type": "integer",
            "label": "Meal Code",
            "readOnly": false,
            "value": "${context.empData.mealCode}",
            "constraints": {
                "required": false
            }
        },
        {
            "id": "job_title",
            "type": "datetime",
            "label": "Date",
            "readOnly": false,
            "value": "${context.empData.orderDate}",
            "constraints": {
                "required": false
            }
        }
    ],
    "decisions": [
        {
            "id": "approve",
            "text": "Approve",
            "decisionType": "positive"
        }
    ]
}